program pGR ;
begin var int32 aAA, bBB, cCC, mAX ;
  read (aAA) 
  read (bBB)
  read (cCC)

  if aAA gt bBB & aAA gt cCC {
    aAA =: mAX
  } else if bBB gt aAA & bBB gt cCC {
    bBB =: mAX
  } else {
    mAX =: cCC
  }
  write (mAX)

  if (aAA == bBB) & (aAA == cCC) & (bBB == cCC) {
    write (1)
  } else {
    write (0)
  }

  if (aAA lt 0) | (bBB lt 0) | (c lt 0) {
    write (-1)
  } else {
    write (0)
  }

  if !(aAA lt (bBB add cCC)) {
    write (10)
  } else {
    write (0)
  }

end
